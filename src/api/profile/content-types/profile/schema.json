{
  "kind": "singleType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "about_descriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::about-description.about-description"
    },
    "position": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "headerImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "instagramLink": {
      "type": "string"
    },
    "linkedinLink": {
      "type": "string"
    },
    "facebookLink": {
      "type": "string"
    }
  }
}
